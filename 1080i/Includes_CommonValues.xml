<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <!-- Defaults -->
    <include name="MediaFlagVarsShelf">
		<width>120</width>
		<height>65</height>
		<aspectratio>keep</aspectratio>
		<fadetime>250</fadetime>
		<colordiffuse>white</colordiffuse>
		<animation effect="fade" time="200">VisibleChange</animation>
	</include>	
	<include name="FullscreenDimensions">
        <posx>0</posx>
        <posy>0</posy>
        <width>1920</width>
        <height>1080</height>
    </include>
	<include name="HiddenObject">
        <posx>-300</posx>
        <posy>-300</posy>
        <width>1</width>
        <height>1</height>
        <texturefocus>-</texturefocus>
        <texturenofocus>-</texturenofocus>
        <label>-</label>
        <font>-</font>
    </include>
    <include name="HiddenList">
        <posx>-100</posx>
        <posy>-100</posy>
        <width>100</width>
        <height>100</height>
        <itemlayout height="100" width="100" />
        <focusedlayout height="100" width="100" />
    </include>
    <!-- Buttons -->
    <include name="ButtonTopbarCommonvalues">
        <height>60</height>
        <width>60</width>
        <align>center</align>
        <aligny>top</aligny>
        <textoffsety>3</textoffsety>
        <font>IconSmall</font>
        <textcolor>grey</textcolor>
        <focusedcolor>white</focusedcolor>
        <texturenofocus>-</texturenofocus>
        <shadowcolor>ff000000</shadowcolor>
        <alttexturenofocus>-</alttexturenofocus>
    </include>
    <include name="ButtonCommonValues">
        <width>400</width>
        <height>60</height>
        <textoffsetx>15</textoffsetx>
        <textcolor>grey</textcolor>
        <focusedcolor>white</focusedcolor>
        <textwidth>300</textwidth>
