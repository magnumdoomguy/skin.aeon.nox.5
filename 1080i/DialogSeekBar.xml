				<animation effect="slide" end="0,-90" start="0,0" time="480">WindowClose</animation>
				<include>Animation_FadedByOSDSettings</include>
				<include condition="Skin.HasSetting(OSD.TopBarHide.Enable) + StringCompare(Skin.String(OSD.TopBarHide.Delay),5)">Animation_FadeOSD5</include>
				<include condition="Skin.HasSetting(OSD.TopBarHide.Enable) + StringCompare(Skin.String(OSD.TopBarHide.Delay),10)">Animation_FadeOSD10</include>
				<include condition="Skin.HasSetting(OSD.TopBarHide.Enable) + IsEmpty(Skin.String(OSD.TopBarHide.Delay))">Animation_FadeOSD15</include>
				<include condition="Skin.HasSetting(OSD.TopBarHide.Enable) + StringCompare(Skin.String(OSD.TopBarHide.Delay),20">Animation_FadeOSD20</include>
				<include condition="Skin.HasSetting(OSD.TopBarHide.Enable) + StringCompare(Skin.String(OSD.TopBarHide.Delay),25">Animation_FadeOSD25</include>
				<include condition="Skin.HasSetting(OSD.TopBarHide.Enable) + StringCompare(Skin.String(OSD.TopBarHide.Delay),30">Animation_FadeOSD30</include>
            
